# Environment variables for Netlify deployment
# These variables will be available during the build process

# Node.js version (Netlify will use this version)
NODE_VERSION=18

# Build environment
NODE_ENV=production

# Vue.js environment variables (prefixed with VITE_)
# These will be available in your Vue app at build time
VITE_APP_NAME="Vue E-commerce"
VITE_APP_VERSION="1.0.0"
VITE_API_BASE_URL="https://fakestoreapi.com"

# Enable source maps for debugging (optional)
VITE_BUILD_SOURCE_MAP=false

# Build optimizations
VITE_BUILD_MINIFY=true
